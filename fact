import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int [] arr = new int[n];
        System.out.println("Enter the number");
        int fact = 1;
        for(int i=1;i<=n;i++)
        {
            fact = fact*i;
           }
           System.out.print("factprial is:" + fact);
}
}
